{
  "name": "eddiejaoude/codeception-zf2-codecoverage",
  "description": "Codeception ZF2 Code Coverage Module",
  "type": "library",
  "authors": [
    {
      "name": "Eddie Jaoude",
      "email": "eddie@jaoudestudios.com"
    }
  ],
  "license": "MIT",
  "keywords": [
    "zend",
    "zend framework 2",
    "zf2",
    "codeception",
    "code coverage"
  ],
  "require": {
    "php": ">=5.3.3",
    "zendframework/zend-di": "2.*",
    "zendframework/zend-eventmanager": "2.*",
    "zendframework/zend-mvc": "2.*",
    "codeception/codeception": "2.*",
    "codeception/c3": "2.*"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*",
    "mockery/mockery": "0.9.*"
  },
  "autoload": {
    "psr-4": {
      "CodeceptionZf2CodeCoverage\\": "src/CodeceptionZf2CodeCoverage"
    },
    "classmap": [
      "Module.php"
    ]
  }
}
